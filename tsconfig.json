{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"baseUrl": ".",
		"experimentalDecorators": true
	},
	"include": [".scripts"]
}
